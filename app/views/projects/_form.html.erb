<%= form_for @project do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<%= f.label :goal %>
	<%= f.number_field :goal %>
	<%= f.label :start_date %>
	<%= f.date_field :start_date %>
	<%= f.label :finish_date %>
	<%= f.date_field :finish_date %>
	<%= f.submit %>

<% end %>

<%= (params[:project][:start_date]).to_datetime.to_date %>