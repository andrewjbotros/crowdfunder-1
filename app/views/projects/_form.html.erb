<%= form_for @project do |f| %>
	<%= render 'alerts/error_message', object: @project %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<%= f.label :goal %>
	<%= f.number_field :goal %>
	<%= f.label :start_date %>
	<%= f.date_field :start_date %>
	<%= f.label :finish_date %>
	<%= f.date_field :finish_date %>
	<%= f.submit %>

<% end %>
