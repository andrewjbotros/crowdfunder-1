<% if object.errors.any? %>
<div id="error_explanation">
  <div class="flash-danger">
    <!-- <button type="button" class="close" data-dismiss="alert">&times;</button> -->
    <ul>
      <% object.errors.full_messages.each do |message| %>
      <li><%= message %></li>
      <% end %>
    </ul>
  </div>
</div>
<% end %>